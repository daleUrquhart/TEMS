<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Pos?>
<?import com.jfoenix.controls.*?>
 
<VBox 
    spacing="15" 
    alignment="CENTER" 
    styleClass="sign-in-box" 
    fx:controller="com.tems.controllers.ScoreController" 
    xmlns="http://javafx.com/javafx"
    xmlns:jfx="http://javafx.com/javafx/8.0.0"
    xmlns:fx="http://javafx.com/fxml" 
    fx:id="mainBox"
    stylesheets="@/styles/styles.css">
    <Label text="Score Application" styleClass="sub-header"/>
    <!-- Score labels / prompts -->
    <VBox fx:id="scoreBox" spacing="10" alignment="TOP_LEFT" styleClass="aud-listing-box">
    
    </VBox> 
    <JFXButton text="Back to Application" onAction="#backToApp" styleClass="toggle-button"/>
    <JFXButton text="Submit" onAction="#submit" styleClass="toggle-button"/>
</VBox> 